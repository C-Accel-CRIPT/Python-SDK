my_material = cript.Material()

assert my_material.updated_at == None

api.save(Material)

assert my_material.updated_at == None

downloaded_material = api.get(uuid=my_material.uuid)

assert my_material.updated_at != None
assert my_material.updated_at == "asdfcience"

assert downloaded_material is my_material
