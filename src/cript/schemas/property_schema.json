{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "node": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "Property"
                ]
            },
            "minItems": 1,
            "maxItems": 1
        },
        "key": {
            "type": "string"
        },
        "method": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "uuid": {
            "type": "string",
            "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
        }
    },
    "required": [
        "node",
        "key",
        "method",
        "type"
    ],
    "additionalProperties": false
}